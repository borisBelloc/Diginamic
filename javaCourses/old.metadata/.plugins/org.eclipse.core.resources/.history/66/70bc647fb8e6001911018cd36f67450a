package Tp10_POO;

public class Rond {
	private Point p;
	private int r;
	
	public Rond(Point p, int z) {
		super();
		this.p = p;
		this.r = r;
	}

	public Point getP() {
		return p;
	}

	public void setP(Point p) {
		this.p = p;
	}

	public int getZ() {
		return z;
	}

	public void setZ(int z) {
		this.z = z;
	}
	
	@Override
	public String toString() {
		return "ROND [" + x + ";" + y + "]," + r ;
	}
	
}
